- if @check_gba
  = content_for(:meta_tags) do
    %meta{ 'http-equiv' => 'refresh', content: "2; URL=#{registrations_gba_status_url}" }

= flash_alert
= flash_notice

- if @check_gba
  .center
    = spinner

= actions do
  - if @registration_unsuccessful || @button_to
    = action_button(@button_label, @button_to, @button_to_options) unless @check_gba
  - unless @no_cancel_to
    = action_button(@cancel_label, @cancel_to, @cancel_to_options)
  - if @previous
    = previous_button(@previous, action: :annie_are_you_okay)
