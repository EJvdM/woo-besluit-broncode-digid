.table
  .table-row
    .table-cell--heading= t('csn')
    .table-cell--heading= t('activated_at')
    .table-cell--heading= t('verification_handled_by')
    .table-cell--heading= t('account_active')
    .table-cell--heading= t('id_corrected')
    .table-cell--heading= t('suspected_fraud_after_verification')
    - if audited
      .table-cell--heading= t('suspected_fraud_after_audit')

  = render(partial: 'verification', collection: verifications, locals: { audited: audited })
