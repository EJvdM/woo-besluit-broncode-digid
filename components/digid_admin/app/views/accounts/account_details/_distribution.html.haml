%h3= I18n.t('accounts.fields.labels.uitgifte')
.form_wrapper
  = field I18n.t('accounts.fields.labels.herkomst'), herkomst_helper(@account.herkomst)
  - if @account.issuer_type == "balie"
    = field I18n.t('accounts.fields.labels.uitgifteproces'), I18n.t('accounts.fields.labels.balie')
    = field t('accounts.fields.labels.balie'), @account.show_front_desk_name
  - elsif @account.issuer_type == "gemeentebalie"
    = field t('accounts.fields.labels.uitgifteproces'), t('accounts.fields.labels.gemeentebalie')
  - elsif @account.issuer_type == "regulier_app"
    = field t('accounts.fields.labels.uitgifteproces'), t('accounts.fields.labels.regulier_app')
  - else
    = field t('accounts.fields.labels.uitgifteproces'), t('accounts.fields.labels.regulier')
