%h1= I18n.t('front_desks.front_desks_overview')
.actions_top= link_to(I18n.t('front_desks.new_front_desk'), new_front_desk_path, class: 'new_button') if can? :create, FrontDesk
.clear
.form_wrapper
  %h5= I18n.t('front_desks.header_filter')
  = form_tag front_desks_path, method: :get do
    = text_field_tag :search, params[:search]
    = submit_button I18n.t('filter_button').html_safe
    .clear
= render 'overview'
