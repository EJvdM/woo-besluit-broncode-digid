= form_for [@webservice, @shared_secret], remote: true do |f|
  = render 'shared/errors', model: @shared_secret
  = hidden_field_tag(:aselect_webservice_id, @aselect_webservice.try(:id))

  = field(f.label(:shared_secret)) do
    = @shared_secret.generate_shared_secret
    = hidden_field_tag 'shared_secret[shared_secret]', @shared_secret.shared_secret
    %a.print_button{ data_print_page: true }= 'Print'
  .actions
    = link_to(t('cancel'), '#', class: 'close_dialog')
    = submit_button(t('.select'))
