%h1
  = t("services.services.overview")
.clear
.form_wrapper.filter= render "searchform"

%div{class: 'form_wrapper'}
  %h2= t("services.services.services")
  = render 'dc/services/overview'

- if @services_in_review.try(:any?) && !params[:q]
  %div{class: 'form_wrapper'}
    %h2= t("services.four_eyes.to_approve")
    = render 'shared/four_eyes_reviews', review_records: @services_in_review, fields: %w(id service_uuid name), headers: %w(services.id services.services.service_id services.services.service_name services.four_eyes.modification_type services.four_eyes.modification_time services.four_eyes.modification_by services.remarks)
