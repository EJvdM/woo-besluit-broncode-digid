%tr
  %td= organization.id
  %td= organization.oin
  %td= organization.name
  %td= render_status_active(organization.status)
  %td= render_status_from(organization.status)
  %td= render_status_until(organization.status)
  %td
    %ul
      - organization.organization_roles.each do |r|
        %li= render_organization_role_type(r)

  - if can? :read, Dc::Organization
    %td.action= four_eyes_show_link(t("services.view"), organization)
  - if can? :update, Dc::Organization
    %td.action= four_eyes_edit_link(t("services.edit"), edit_dc_organization_path(organization), organization)
