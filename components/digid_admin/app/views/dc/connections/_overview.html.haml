- if @connections.any?
  %table
    %thead
      %tr
        = render 'shared/table_header', headers: %w(services.id services.connections.name services.connections.entity_id services.date_from services.date_until services.connections.organization.name services.connections.organization.oin)
        - if can? :read, Dc::Connection
          %th.action
        - if can? :update, Dc::Connection
          %th.action
    %tbody
      = render @connections.content
    = table_footer 10 do
      = paginate(@connections, param_name: :page_service)
- else
  = "Er konden geen aansluitingen gevonden worden."
