%tr
  %td= whitelisted_phone_number.id
  %td= whitelisted_phone_number.phone_number
  %td= whitelisted_phone_number.description
  %td= I18n.l(whitelisted_phone_number.created_at)
  %td.action= four_eyes_edit_link(I18n.t('edit'), edit_whitelisted_phone_number_url(whitelisted_phone_number), whitelisted_phone_number) if can?(:update, WhitelistedPhoneNumber)
  %td.action
    - if in_review?(whitelisted_phone_number) || is_frozen?(whitelisted_phone_number)
      %span.disabled= t('whitelisted_phone_number.destroy')
    - else
      = link_to(t('whitelisted_phone_number.destroy'), whitelisted_phone_number_path(whitelisted_phone_number), data: {disabled: true, confirm: t('are_you_sure') }, method: :delete) if can?(:destroy, WhitelistedPhoneNumber)
