.form_wrapper
  %h1= "RDA CRL #{@crl.id}"
  .actions_top
    = link_to('Terug naar overzicht', rda_crl_index_path)
    = link_to('Download', download_rda_crl_path(@crl)) if can?(:manage, @crl)
  .clear
  = field 'ID', @crl.id
  = field 'Uitgever', @crl.issuer
  = field 'Gemaakt op', l(@crl.this_update)
  = field 'Geldig tot', l(@crl.next_update)
  .clear
%br
= show_manager_actions_for('Rda::Crl', @crl.id)
