%tr
  %td= third_party_app.id
  %td= third_party_app.user_agent
  %td= third_party_app.return_url
  %td= localize(third_party_app.created_at)
  %td= localize(third_party_app.updated_at)
  - if can? :update, ThirdPartyApp
    %td.action= four_eyes_edit_link t('edit'), edit_third_party_app_path(third_party_app), third_party_app
  %td.action
    - if in_review?(third_party_app) || is_frozen?(third_party_app)
      %span.disabled= t('third_party_app.destroy')
    - else
      = link_to(t('third_party_app.destroy'), third_party_app_path(third_party_app), data: {disabled: true, confirm: t('are_you_sure_third_party_app') }, method: :delete) if can?(:destroy, ThirdPartyApp)
