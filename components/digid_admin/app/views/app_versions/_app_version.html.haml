%tr
  %td= app_version.id
  %td= app_version.version
  %td= t(app_version.operating_system, scope: 'app_version.operating_systems')
  %td= app_version.release_type
  %td= localize(app_version.not_valid_before)
  %td= localize(app_version.not_valid_on_or_after)
  %td= localize(app_version.kill_app_on_or_after)
  - if can? :update, AppVersion
    %td.action= four_eyes_edit_link t('edit'), edit_app_version_path(app_version), app_version
